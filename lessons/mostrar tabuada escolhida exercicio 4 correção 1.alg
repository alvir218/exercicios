algoritmo "exercicio 4"

var

  i,f,c,ti,tf,t:inteiro

inicio

  escreva("tabuada inicial:")
  leia(ti)
  escreva("tabuada final:")
  leia(tf)
  escreva("limite inicial:")
  leia(i)
  escreva("limite final:")
  leia(f)
  para t de ti ate tf faca
    para c de i ate f faca
      escreval(t," x",c,"=",t*c)
    fimpara
    escreval
  fimpara
fimalgoritmo
